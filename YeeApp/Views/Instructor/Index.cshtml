@model IEnumerable<Instructor>;




<table class="table table-dark">

    <tr>
        <th colspan="6">Instructor</th>
    </tr>

    <tr>
        <td>Name</td>
        <td>Rank</td>
        <td>Hiring Date</td>
        <td>Is Tenured</td>
        <td>More Details</td>
        <td>Change Details</td>


    </tr>
    @foreach (var instructor in Model)
    {
        <tr>
            <td>@instructor.FirstName @instructor.LastName</td>
            <td>@instructor.Rank</td>
            <td>@instructor.HiringDate</td>
            <td>@instructor.IsTenured</td>
            <td><a asp-action="ShowDetail" asp-route-id="@instructor.Id">Show Details</a></td>
            <td><a asp-action="EditDetails" asp-controller="Instructor" asp-route-id="@instructor.Id">Edit Details</a></td>

        </tr>
    }
        <button><a asp-action="AddInstructor" asp-controller="Instructor">Add Instructor</a></button>

    

    
</table>